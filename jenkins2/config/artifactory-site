<VirtualHost *:80>
  ServerAdmin webmaster@localhost
  ServerName artifactory.jenkins.com
  ServerAlias artifactory
  ProxyRequests Off
  <Proxy *>
    Order deny,allow
    Allow from all
  </Proxy>
  ProxyPreserveHost on
  ProxyPass / http://localhost:8081/ nocanon
  AllowEncodedSlashes NoDecode

  ErrorLog ${APACHE_LOG_DIR}/artifactory-error.log
  LogLevel warn
  CustomLog ${APACHE_LOG_DIR}/artifactory-access.log combined

</VirtualHost>